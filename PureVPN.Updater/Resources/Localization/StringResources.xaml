<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="Cancel">Cancel download</system:String>
    <system:String x:Key="Downloading update">Downloading update...</system:String>
    <system:String x:Key="Downloading update description">A new version of PureVPN is downloading now! Please find the release notes mentioned below.</system:String>
    <system:String x:Key="Minimize">Minimize</system:String>

</ResourceDictionary>